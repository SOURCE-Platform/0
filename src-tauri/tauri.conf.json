{
  "productName": "observer-app",
  "version": "0.1.0",
  "identifier": "com.tauri.dev",
  "build": {
    "beforeBuildCommand": "sh -c 'pwd && ls -la . && cd nextjs-app && npm run build'",
    "beforeDevCommand": "sh -c 'pwd && ls -la . && cd nextjs-app && npm run dev'",
    "devUrl": "http://localhost:3000",
    "frontendDist": "../nextjs-app/out"
  },
  "app": {
    "windows": [
      {
        "fullscreen": false,
        "height": 600,
        "resizable": true,
        "title": "Observer App",
        "width": 800
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "category": "DeveloperTool",
    "copyright": "",
    "externalBin": [],
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ],
    "longDescription": "",
    "macOS": {
      "entitlements": null,
      "exceptionDomain": "",
      "frameworks": [],
      "providerShortName": null,
      "signingIdentity": null
    },
    "resources": [],
    "shortDescription": "",
    "targets": "all",
    "windows": {
      "certificateThumbprint": null,
      "digestAlgorithm": "sha256",
      "timestampUrl": ""
    }
  }
}
